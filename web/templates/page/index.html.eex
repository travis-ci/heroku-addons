<div class="jumbotron">
  <h2>Travis CI Heroku meta-dashboard</h2>
</div>

<div class="row marketing">
  <div class="col-lg-6">
    <h4>Apps</h4>
    <ul>
      <%= for app <- @apps do %>
      <li>
        <%= app.name %>
        <ul>
          <%= for addon <- app.addons do %>
          <li><%= addon.name %></li>
          <% end %>
        </ul>
      </li>
      <% end %>
    </ul>
  </div>
</div>
